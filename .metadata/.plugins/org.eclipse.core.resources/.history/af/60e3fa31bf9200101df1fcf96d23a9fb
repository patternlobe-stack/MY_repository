<script>


$(()=>{
		$('#btn1').on('click', ()=>{
			//img태그 만들기 
		vimg =$(`<img src='../images/가구1.jpg' alt=' 가구1.jpg'>` )
		
	//	$('#result1').append(vimg);
			$(vimg).appendTo($('#result1'));
		})
$('#btn2').on('click', ()=>{
	
	//img태그 만들기
	vimg = $(`<img src='../images/국화1.jpg' alt='국화1.jpg'>`);
	$('#result2').prepend(vimg)

	$(vimg).prependTo()

})
		
})

</script>

