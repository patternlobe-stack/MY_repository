<%
//êº¼ë‚´ê¸° 
List<BoardVO>list = (List<BoardVO>)request.getAttribute("boardList");
PageInfo pinfo=(PageInfo)request.getAttribute("pinfo");

JsonObject job = new JsonObject();
job.addProperty("sp", pinfo.getStartPage());
job.addProperty("ep", pinfo.getEndPage());
job.addProperty("tp", pinfo.getTotalPage());


Gson gson =new Gson();
JsonElement blist = gson.toJsonTree(list);


%>